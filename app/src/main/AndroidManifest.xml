<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="theitcloud.example.com.justgnosh">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LocalRestaurants"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Menu"
            android:parentActivityName=".LocalRestaurants" />
        <activity
            android:name=".RestaurantMenuActivity"
            android:label="@string/title_activity_restaurant_menu" />
        <!--
        <activity android:name=".FragmentActivity" />
        <activity android:name=".Activity" />
        <activity android:name=".MenuActivity"></activity>
        -->
        <activity
            android:name=".SevenBone"
            android:parentActivityName=".RestaurantMenuActivity" />
        <activity
            android:name=".Franco"
            android:parentActivityName=".RestaurantMenuActivity" />
        <activity
            android:name=".Moose"
            android:parentActivityName=".RestaurantMenuActivity" />
        <activity
            android:name=".BasketActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".Login"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".SevenBoneJuicyDogs" />
        <activity
            android:name=".Register"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".WaitTime" />
        <activity android:name=".FrancoPizza"></activity>
    </application>

</manifest>